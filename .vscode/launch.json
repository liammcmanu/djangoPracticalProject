{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Django: Runserver",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
				"runserver"
			],
			"django": true
		},
		{
			"name": "Django: Test",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
				"test",
				"testing/tests"
			],
			"django": true
		},
		{
			"name": "Django: Migrate",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
				"migrate"
			],
			"django": true
		},
		{
			"name": "Django: Makemigrations",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/manage.py",
			"args": [
				"makemigrations"
			],
			"django": true
		},
	],
}